{
	"name": "Solarized Dark Extended",
	"tokenColors": [
		{
			"settings": {
				"foreground": "#93A1A1"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#2AA198"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#586E75"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#269186"
			}
		},		
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#D30102"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#269186"
			}
		},
		{
			"name": "Variable",
			"scope": [
				// this, window, etc.
				"variable.language",
				// ${} template literal 
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.end.js",
				"meta.template.expression.js",
				"meta.embedded.line.js"
			],
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"name": "Variable Function",
			"scope": [
				"variable.function.js",
				"string.interpolated"
			],
			"settings": {
				"foreground": "#ff00cc"
			}
		},
		{
			"name": "Variable Other",
			"scope": [
				"variable.other",
				"variable.other.member"
			],
			"settings": {
				"foreground": "#93A1A1"
			}
		},		
		{
			"name": "Keyword",
			"scope": ["keyword", 
				"punctuation.definition.typeparameters.begin.tsx",
				"punctuation.definition.typeparameters.end.tsx"
			],
			"settings": {
				"foreground": "#859900"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#738A05"
			}
		},
		// {
		// 	"name": "Class name",
		// 	"scope": [
		// 		"entity.name.class",
		// 		"entity.name.type"
		// 	],
		// 	"settings": {
		// 		"fontStyle": "",
		// 		"foreground": "#268BD2"
		// 	}
		// },		
		{
			"name": "Variable start",
			"scope": ["punctuation.definition.variable"],
			"settings": {
				"foreground": "#859900"
			}
		},
		{
			"name": "Embedded code markers",
			"scope": [
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#269186"
			}
		},
		{
			"name": "Built-in constant",
			"scope": [
				// true, false, Infinity etc.
				"constant.language",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#B58900"
			}
		},
		{
			"name": "Support.construct",
			"scope": [
				"support.function.construct",
				"keyword.other.new"
			],
			"settings": {
				"foreground": "#D30102"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#D30102"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#93A1A1"
			}
		},
		{
			"name": "Function argument",
			"scope": ["variable.parameter", "meta.type.parameters.tsx", "support.type.primitive.tsx"],
			"settings": {
				"foreground": "#A57800"
			}
		},
		{
			"name": "import from keyword",
			"scope": [
				"keyword.control.from.js",
				"meta.object-literal.key.js",
				"punctuation.separator.comma.js",
				"meta.function.expression.js",
				"meta.function-call.js",
				"source.js",
				"meta.function-call.tsx"
			],
			"settings": {
				"foreground": "#93A1A1"
			}
		},
		// {
		// 	"name": "Function call",
		// 	"scope": [
		// 		// JSON, plugins etc.
		// 		"meta.function-call",
		// 		"meta.function-call.arguments"
		// 	],
		// 	"settings": {
		// 		"foreground": "#93A1A1"
		// 	}
		// },
		{
			"name": "Tag name",
			"scope": "entity.name.tag", // HTML, JSP Tags
			"settings": {
				"fontStyle": "bold",
				"foreground": "#268BD2"
			}
		},
		{
			"name": "Tag start/end",
			"scope": "punctuation.definition.tag", // JSTL < and />
			"settings": {
				"foreground": "#586E75"
			}
		},
		{
			"name": "Tag attribute name",
			"scope": "entity.other.attribute-name", // ex. attrname=""
			"settings": {
				"foreground": "#93A1A1"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function", // Native functions, like push, pop etc.
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"name": "Continuation",
			"scope": "punctuation.separator.continuation",
			"settings": {
				"foreground": "#D30102"
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type", // Array, Object, keys in JSON
				"support.class"
			],
			"settings": {
				"foreground": "#859900"
			}
		},
		{
			"name": "Library Exception",
			"scope": [
				"support.type.exception"
			],
			"settings": {
				"foreground": "#CB4B16"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string.quoted.double",
				"string.quoted.single",
				"string.template.js"
			],
			"settings": {
				"foreground": "#269186"
			}
		},	
		{
			"name": "Quotes",
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#93A1A1"
			}
		},				
